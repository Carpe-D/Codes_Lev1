def solution(N,stages):
    answer=[]
    failure=[]
    count=0
    for i in range(1,N+1):
        for k in stages:
            if i<=k:
                count+=1
        if count==0:
            failure.append(0)
        else:
            failure.append(stages.count(i)/count)
            count=0
    for k in range(N):
        max_index=failure.index(max(failure))
        answer.append(max_index+1)
        failure[max_index]=-1
    return answer
